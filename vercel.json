{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install --legacy-peer-deps",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/public/index.html"
        }
    ]
}
